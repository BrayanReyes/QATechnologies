<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Booking.com Test" verbose="1">
	<listeners>
		<listener
			class-name="org.bookingSuite.web.listeners.Listener"></listener>
	</listeners>

	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url"
		value="https://www.booking.com/index.es.html"></parameter>

	<test name="Search Stay and enter Payment Data">
		<classes>
			<class name="org.bookingSuite.web.tests.BookingTest"></class>
		</classes>
	</test>
</suite>