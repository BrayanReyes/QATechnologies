<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Test ESPN" verbose="1">
	<listeners>
		<listener class-name="org.bookingSuite.web.listeners.Listener"></listener>
	</listeners>
	
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url"	value="https://www.booking.com/index.es.html"></parameter>

	<test name="Log In Test ESPN Web Page">
		<classes>
			<class name="org.bookingSuite.web.tests.BookingTest"></class>
		</classes>
	</test>
</suite>